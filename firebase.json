{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
"rewrites": [
        {
        "source": "/",
        "run": {
          "serviceId": "website",
          "region": "us-central1"
        }
      },
      {
        "source": "**",
        "run": {
          "serviceId": "website",
          "region": "us-central1"
        }
      }
    ]
  }
}
